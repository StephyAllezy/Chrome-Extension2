{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "permissions": ["tabs", "activeTab", "storage"],
  "version": "1.0",
  "action": {
    "default_icon": "flower.png"
  },
  "content_scripts": [
    {
      "js": ["contents.js"],
      "matches": ["https://www.google.com/*"],
      "css": ["contents.css"]
    }
  ]
}
