{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "flower.png"
  },
  "content_script": [
    {
      "js": ["scripts/content.js"],
      "matches": ["all_urls"]
    }
  ]
}
